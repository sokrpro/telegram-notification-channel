{
    "name": "sokrpro/telegram-notification-channel",
    "description": "Telegram Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "telegram"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maksym Maksymenko",
            "email": "6aff21@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.9",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^8.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sokrpro\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sokrpro\\Tests\\Notifications\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sokrpro\\Notifications\\TelegramChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
